PROC 0 LIST SYMLIST CONLIST  OUTPUT(JCL800) +
        DSN(CBT.CNTL)

        IF &SYMLIST ¬= THEN CONTROL SYM
        IF &CONLIST ¬= THEN CONTROL CON
        IF &LIST    ¬= THEN CONTROL LIST
            FREE FILE(OUT)
            WRITE GENERATING JCL IN &DSN(&OUTPUT).
            ALLOC F(OUT) DA(&DSN(&OUTPUT)) SH  REU
            OPENFILE OUT OUTPUT
        SET &FL = 701
        DO UNTIL &FL = 801
            SET &OUT = &STR(//S)&FL&STR( EXEC NULL,N=)&FL&STR(,TO='FILE-
0)&FL&STR(')
            PUTFILE OUT
            SET &FL = &FL + 1
        END
            CLOSFILE OUT
            FREE FILE(OUT)
