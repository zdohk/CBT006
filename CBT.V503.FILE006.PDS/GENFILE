PROC 0 LIST SYMLIST CONLIST  OUTPUT(NEW700) +
        DSN(CBT.CNTL)

        IF &SYMLIST ¬= THEN CONTROL SYM
        IF &CONLIST ¬= THEN CONTROL CON
        IF &LIST    ¬= THEN CONTROL LIST
            FREE FILE(OUT)
            WRITE GENERATING JCL IN &DSN(&OUTPUT).
            ALLOC F(OUT) DA(&DSN(&OUTPUT)) SH  REU
            OPENFILE OUT OUTPUT
        SET &FL = 601
        DO UNTIL &FL = 801
            SET &OUT = &STR(./ ADD NAME=@FILE&FL 0100-02160-02160-1200--
00002-00002-00000-CBT449)
            PUTFILE OUT
            SET &HYT = &STR(FILE &FL)
            SET &OUT = &STR(//***)&HYT&STR( IS CURRENTLY EMPTY         -
                           *   )&HYT
            PUTFILE OUT
            SET &OUT = &STR(//*                                        -
                         *   )&HYT
            PUTFILE OUT
            SET &FL = &FL + 1
        END
            CLOSFILE OUT
            FREE FILE(OUT)
